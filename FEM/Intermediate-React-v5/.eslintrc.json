{
    "extends":[
        "eslint:recommended",
        "prettier"
    ],
    "plugins": [],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-case-declarations": 0,
        "no-unexpected-multiline": "warn",
        "no-debugger": "warn",
        "react/prop-types": 0,
        "react/jsx-key": 0,
        "no-unused-vars": "off"
    }
}